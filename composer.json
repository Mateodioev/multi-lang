{
    "name": "mateodioev/multi-lang",
    "description": "A simple PHP class to manage multi-language websites.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "multi-language",
        "language",
        "translation",
        "translate",
        "i18n",
        "l10n"
    ],
    "autoload": {
        "psr-4": {
            "Mateodioev\\MultiLang\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --config=.php-cs-fixer.dist.php -vv"
    }
}
